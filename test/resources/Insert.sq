data IList where
  Nil :: IList 
  Cons :: x: {Int| |2} -> xs: IList -> IList

measure numgt :: x: Int -> IList -> {Int| _v >= 0} where 
  Nil -> 0 
  Cons y ys -> 
    if y >= x 
      then 1 + (numgt x ys)
      else numgt x ys
             
measure count2 :: x: Int -> y: Int -> IList -> {Int | _v >= 0} where 
  Nil -> 0 
  Cons z zs -> 
    if (z == x || z == y)
      then 1 + (count2 x y zs) 
      else count2 x y zs