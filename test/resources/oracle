compress :: xs:{List {1**a||1}||0} -> {List {1**a||0}||0}
compress = \xs . 
  match xs with
    Nil -> Nil
    Cons y ys -> 
      match compress ys with
        Nil -> Cons y Nil
        Cons z zs -> 
          if y == z
            then Cons z zs
            else Cons y (Cons z zs)

